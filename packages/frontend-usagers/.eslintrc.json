{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["@nuxt/eslint-config", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn"],
    "vue/multi-word-component-names": "off"
  },
  "ignorePatterns": ["src/public"]
}
