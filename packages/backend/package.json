{
  "name": "@vao/backend",
  "version": "0.1.0",
  "description": "Backend du projet VAO",
  "author": "orgs/SocialGouv/teams/vao",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts ./src",
    "lint:fix": "corepack pnpm lint --fix",
    "dev": "nodemon --inspect=0.0.0.0:9229 src/index.js",
    "test": "corepack pnpm rebuild @sentry/profiling-node && jest --config=jest.config.js",
    "test:debug": "corepack pnpm rebuild @sentry/profiling-node && jest --config=jest.config.js --debug --detectOpenHandles",
    "test:watch": "corepack pnpm rebuild @sentry/profiling-node && jest --config=jest.config.js --watch",
    "test:coverage": "corepack pnpm rebuild @sentry/profiling-node && jest --config=jest.config.js --coverage",
    "build": "tsc -b ./tsconfig.json"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "corepack pnpm exec eslint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.649.0",
    "@sentry/node": "^8.49.0",
    "@sentry/profiling-node": "^8.49.0",
    "@vao/shared-bridge": "workspace:^",
    "axios": "^1.7.4",
    "body-parser": "~1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "~2.8.5",
    "cron": "~3.1.7",
    "dayjs": "^1.11.10",
    "debug": "~4.3.4",
    "express": "~4.21.0",
    "file-type": "^19.6.0",
    "form-data": "^4.0.1",
    "helmet": "~8.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pdfmake": "^0.2.10",
    "pg": "~8.13.0",
    "pg-format": "~1.0.4",
    "proj4": "^2.15.0",
    "ufo": "^1.5.4",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "^1.131.0",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.15.5",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "nodemon": "^3.1.0",
    "pdf-lib": "^1.17.1",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
