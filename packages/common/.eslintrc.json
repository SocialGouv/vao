{
  "root": true,
  "env": {
      "browser": true,
      "node": true,
      "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "@nuxt/eslint-config",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
  },
  "rules": {
      "prettier/prettier": [
          "error"
      ],
      "vue/multi-word-component-names": "off"
  }
}